
// This program is a simple test of conditionals, including nesting

INT FUNC while_test ()
START
    INT a; INT b;
    a := 10;
    b := -15;
    IF ( a-10 ) THEN
        PRINT "A is different from 10";
    END
    IF ( a ) THEN
        IF ( b+15 ) THEN
            PRINT "B is different from -15";
        ELSE
            PRINT "B equals -15";
        END
    END
    RETURN 0;
END
