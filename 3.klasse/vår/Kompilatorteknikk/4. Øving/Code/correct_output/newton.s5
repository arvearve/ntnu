 PROGRAM()()
 FUNCTION_LIST()()
  FUNCTION(INTEGER)("newton")()
   VARIABLE_LIST()()
    DECLARATION_STATEMENT(INTEGER)("n")()
   STATEMENT_LIST()()
    DECLARATION_STATEMENT(INTEGER)("square_root")()
    ASSIGNMENT_STATEMENT()()
     VARIABLE()("square_root")()
     EXPRESSION()(FUNC_CALL)
      VARIABLE()("improve")()
      EXPRESSION_LIST()()
       VARIABLE()("n")()
       CONSTANT(INTEGER)(1)()
    PRINT_STATEMENT()()
     EXPRESSION_LIST()()
      CONSTANT(STRING)("The square root of")()
      VARIABLE()("n")()
      CONSTANT(STRING)("is")()
      VARIABLE()("square_root")()
    RETURN_STATEMENT()()
     CONSTANT(INTEGER)(0)()
  FUNCTION(INTEGER)("improve")()
   VARIABLE_LIST()()
    DECLARATION_STATEMENT(INTEGER)("n")()
    DECLARATION_STATEMENT(INTEGER)("estimate")()
   STATEMENT_LIST()()
    DECLARATION_STATEMENT(INTEGER)("next")()
    ASSIGNMENT_STATEMENT()()
     VARIABLE()("next")()
     EXPRESSION()(-)
      VARIABLE()("estimate")()
      EXPRESSION()(/)
       EXPRESSION()(-)
        EXPRESSION()(*)
         VARIABLE()("estimate")()
         VARIABLE()("estimate")()
        VARIABLE()("n")()
       EXPRESSION()(*)
        CONSTANT(INTEGER)(2)()
        VARIABLE()("estimate")()
    IF_STATEMENT()()
     EXPRESSION()(-)
      VARIABLE()("next")()
      VARIABLE()("estimate")()
     STATEMENT_LIST()()
      RETURN_STATEMENT()()
       EXPRESSION()(FUNC_CALL)
        VARIABLE()("improve")()
        EXPRESSION_LIST()()
         VARIABLE()("n")()
         VARIABLE()("next")()
     STATEMENT_LIST()()
      RETURN_STATEMENT()()
       EXPRESSION()(-)
        VARIABLE()("next")()
        CONSTANT(INTEGER)(1)()
